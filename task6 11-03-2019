        <script type="text/javascript">
            var video=document.getElementById('videoPlayer');
            function play(){
                video.play();
            }
            function pause(){
                video.pause();
            }
            function stop(){
                video.stop();
                video.currentTime=0;
            }
            function updateProgress(){
                var pb=document.getElementById('progressbar');
                pb.innerHtml=Math.round((video.currentTime*100)/video.duration);
            }

        </script>
